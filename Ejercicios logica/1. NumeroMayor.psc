Algoritmo NumeroMayor
	
	Definir num1,num2 Como Entero;
	
	Escribir ("Ingresa primer numero");
	leer num1;
	
	Escribir ("Ingresa segundo numero");
	leer num2;
	
	Si num1==num2 Entonces
		Escribir "Los numeros ", num1 , " y " , num2 , " son iguales";
	SiNo
		si num1>num2 Entonces
			Escribir "El primer numero es mayor: ", num1, " que el segundo: " , num2;
		SiNo
				Escribir "El primer numero es menor: ", num1 , " que el segundo " , num2;
			FinSi
	
	Fin Si
	
	
	
	
	
	
FinAlgoritmo
